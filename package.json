{
  "name": "moon",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "build": "packemon build --addEngines --addExports",
    "build:cli": "node ./scripts/buildBinary.mjs",
    "pack": "NODE_ENV=production yarn run packemon pack --addEngines --declaration=standard",
    "format": "prettier --write ./packages",
    "test": "jest --passWithNoTests",
    "lint": "eslint --cache --color --fix --ext .ts,.tsx ./packages",
    "type": "tsc --build",
    "artifacts": "napi artifacts",
    "compile": "yarn run compile:debug --release",
    "compile:debug": "napi build --platform --features cli --cargo-cwd ./crates/cli --js-package-name @moonrepo/core --js ./packages/cli/index.js --dts ./packages/cli/index.d.ts",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "cli": "moon"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "devDependencies": {
    "@moonrepo/cli": "workspace:*",
    "@napi-rs/cli": "^2.4.4",
    "@types/node": "^17.0.15",
    "eslint": "^8.8.0",
    "eslint-config-beemo": "^1.2.2",
    "jest": "^27.4.7",
    "jest-preset-beemo": "^1.1.5",
    "packemon": "^1.11.1",
    "prettier": "^2.5.1",
    "prettier-config-beemo": "^1.0.1",
    "tsconfig-beemo": "^1.0.1",
    "typescript": "^4.5.5"
  },
  "napi": {
    "name": "moon",
    "triples": {
      "default": false,
      "additional": [
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  }
}
