{
  "name": "@moonrepo/cli",
  "version": "0.7.0",
  "type": "commonjs",
  "description": "moon command line and core system.",
  "keywords": [
    "moon",
    "repo",
    "cli",
    "core"
  ],
  "types": "./dts/index.d.ts",
  "main": "./cjs/index.cjs",
  "files": [
    "bin/**/*",
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "moon",
    "postinstall.js",
    "src/**/*.{ts,tsx,json}"
  ],
  "author": "Miles Johnson",
  "license": "MIT",
  "bin": {
    "moon": "moon",
    "moon-run-script": "./bin/moon-run-script.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moonrepo/moon",
    "directory": "packages/cli"
  },
  "scripts": {
    "postinstall": "node ./postinstall.js"
  },
  "dependencies": {
    "detect-libc": "^2.0.1"
  },
  "optionalDependencies": {
    "@moonrepo/core-linux-x64-gnu": "^0.7.0",
    "@moonrepo/core-linux-x64-musl": "^0.7.0",
    "@moonrepo/core-macos-arm64": "^0.7.0",
    "@moonrepo/core-macos-x64": "^0.7.0",
    "@moonrepo/core-windows-x64-msvc": "^0.7.0"
  },
  "packemon": {
    "format": "cjs",
    "platform": "node"
  }
}
