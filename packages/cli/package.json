{
  "name": "@moonrepo/cli",
  "version": "0.1.0",
  "description": "Moon command line and core system.",
  "keywords": [
    "moon",
    "repo",
    "cli",
    "core"
  ],
  "author": "Miles Johnson",
  "license": "MIT",
  "type": "module",
  "main": "./mjs/index.mjs",
  "module": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/milesj/moon",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "napi build --release",
    "build:debug": "napi build"
  },
  "napi": {
    "name": "moon"
  },
  "dependencies": {
    "detect-libc": "^2.0.1"
  },
  "optionalDependencies": {
    "@moonrepo/core-linux-x64-gnu": "^0.1.0"
  },
  "packemon": {
    "format": "mjs",
    "platform": "node",
    "bundle": true
  },
  "files": [
    "*.node",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ]
}
