{
  "name": "@moonrepo/cli",
  "version": "0.1.0",
  "description": "Moon command line and core system.",
  "keywords": [
    "moon",
    "repo",
    "cli",
    "core"
  ],
  "os": [
    "linux",
    "win32",
    "darwin"
  ],
  "cpu": [
    "x64"
  ],
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": {
    "moon": "moon"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milesj/moon",
    "directory": "packages/cli"
  },
  "scripts": {
    "postinstall": "node ./lib/postinstall.js"
  },
  "dependencies": {
    "detect-libc": "^2.0.1"
  },
  "optionalDependencies": {
    "@moonrepo/core-darwin-x64": "^0.1.0",
    "@moonrepo/core-linux-x64-gnu": "^0.1.0",
    "@moonrepo/core-linux-x64-musl": "^0.1.0"
  },
  "packemon": {
    "format": "lib",
    "platform": "node"
  },
  "files": [
    "*.node",
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "moon",
    "moon.exe",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "node": "./lib/*.js"
    },
    ".": {
      "node": "./lib/index.js"
    }
  }
}
