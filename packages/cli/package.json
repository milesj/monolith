{
  "name": "@moonrepo/cli",
  "version": "0.1.0",
  "description": "Moon command line and core system.",
  "keywords": [
    "moon",
    "repo",
    "cli",
    "core"
  ],
  "os": [
    "linux",
    "win32",
    "darwin"
  ],
  "cpu": [
    "x64"
  ],
  "author": "Miles Johnson",
  "license": "MIT",
  "type": "module",
  "main": "./mjs/index.mjs",
  "module": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/milesj/moon",
    "directory": "packages/cli"
  },
  "napi": {
    "name": "moon",
    "triples": {
      "default": false,
      "additional": [
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "dependencies": {
    "detect-libc": "^2.0.1"
  },
  "optionalDependencies": {
    "@moonrepo/core-darwin-x64": "^0.1.0",
    "@moonrepo/core-linux-x64-gnu": "^0.1.0",
    "@moonrepo/core-linux-x64-musl": "^0.1.0"
  },
  "packemon": {
    "format": "mjs",
    "platform": "node",
    "bundle": true
  },
  "files": [
    "*.node",
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  }
}
