{
  "extends": "../tsconfig.options.json",
  "compilerOptions": {
    "outDir": "../.moon/cache/types/website",
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ],
    "baseUrl": ".",
    "jsx": "react",
    "lib": [
      "dom"
    ],
    "paths": {
      "@site/*": [
        "./*"
      ]
    }
  },
  "include": [
    ".eslintrc.js",
    "*.config.js",
    "src/**/*"
  ],
  "references": [
    {
      "path": "../packages/runtime"
    },
    {
      "path": "../packages/types"
    }
  ]
}
